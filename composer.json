{
  "name":         "laravel/lumen",
  "description":  "The Laravel Lumen Framework.",
  "keywords":     [
    "framework",
    "laravel",
    "lumen"
  ],
  "license":      "MIT",
  "type":         "project",
  "require":      {
    "laravel/lumen-framework": "5.0.*",
    "vlucas/phpdotenv":        "~1.0"
  },
  "require-dev":  {
    "phpunit/phpunit":             "~4.0",
    "barryvdh/laravel-ide-helper": "^2.0",
    "doctrine/dbal":               "^2.5",
    "filp/whoops":                 "^1.1",
    "laracasts/testdummy":         "^2.3",
    "fzaninotto/faker":            "^1.4"
  },
  "autoload":     {
    "psr-4":    {
      "App\\": "app/"
    },
    "classmap": [
      "database/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "config":       {
    "preferred-install": "dist"
  },
  "scripts":      {
    "post-update-cmd": [
      "php artisan ide-helper:meta"
    ]
  }
}
